{
  "name": "@capraconsulting/webapp-deploy-lambda",
  "version": "0.0.0-development",
  "description": "CDK construct for deploying a webapp release to S3 and CloudFront",
  "repository": {
    "type": "git",
    "url": "https://github.com/capraconsulting/webapp-deploy-lambda"
  },
  "scripts": {
    "build": "./build.sh && tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build && husky install",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "cdk",
    "cloudfront",
    "deployment",
    "s3",
    "webapp"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/webapp_deploy/**/*",
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.107.0",
    "@aws-cdk/aws-cloudfront": "1.107.0",
    "@aws-cdk/aws-iam": "1.107.0",
    "@aws-cdk/aws-lambda": "1.107.0",
    "@aws-cdk/aws-s3": "1.107.0",
    "@aws-cdk/aws-s3-assets": "1.107.0",
    "@aws-cdk/core": "1.107.0",
    "@aws-cdk/custom-resources": "1.107.0",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.2",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "jest": "27.0.3",
    "jest-cdk-snapshot": "1.4.1",
    "prettier": "2.3.0",
    "semantic-release": "17.4.3",
    "ts-jest": "27.0.2",
    "ts-node": "10.0.0",
    "typescript": "4.3.2"
  },
  "peerDependencies": {
    "@aws-cdk/aws-cloudfront": "^1.45.0",
    "@aws-cdk/aws-iam": "^1.45.0",
    "@aws-cdk/aws-lambda": "^1.45.0",
    "@aws-cdk/aws-s3": "^1.45.0",
    "@aws-cdk/aws-s3-assets": "^1.45.0",
    "@aws-cdk/core": "^1.45.0",
    "@aws-cdk/custom-resources": "^1.45.0"
  }
}
