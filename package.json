{
  "name": "@capraconsulting/webapp-deploy-lambda",
  "version": "0.0.0-development",
  "description": "CDK construct for deploying a webapp release to S3 and CloudFront",
  "repository": {
    "type": "git",
    "url": "https://github.com/capraconsulting/webapp-deploy-lambda"
  },
  "scripts": {
    "build": "./build.sh && tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "cdk",
    "cloudfront",
    "deployment",
    "s3",
    "webapp"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/webapp_deploy/**/*",
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.85.0",
    "@aws-cdk/aws-cloudfront": "1.85.0",
    "@aws-cdk/aws-iam": "1.85.0",
    "@aws-cdk/aws-lambda": "1.85.0",
    "@aws-cdk/aws-s3": "1.85.0",
    "@aws-cdk/aws-s3-assets": "1.85.0",
    "@aws-cdk/custom-resources": "1.85.0",
    "@aws-cdk/core": "1.85.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/jest": "26.0.20",
    "@types/node": "13.13.40",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-deprecation": "1.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-cdk-snapshot": "1.4.1",
    "prettier": "2.2.1",
    "semantic-release": "17.3.1",
    "ts-jest": "26.4.4",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "@aws-cdk/aws-cloudfront": "^1.45.0",
    "@aws-cdk/aws-iam": "^1.45.0",
    "@aws-cdk/aws-lambda": "^1.45.0",
    "@aws-cdk/aws-s3": "^1.45.0",
    "@aws-cdk/aws-s3-assets": "^1.45.0",
    "@aws-cdk/custom-resources": "^1.45.0",
    "@aws-cdk/core": "^1.45.0"
  }
}
