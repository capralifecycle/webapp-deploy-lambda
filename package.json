{
  "name": "@capraconsulting/webapp-deploy-lambda",
  "version": "0.0.0-development",
  "description": "CDK construct for deploying a webapp release to S3 and CloudFront",
  "repository": {
    "type": "git",
    "url": "https://github.com/capraconsulting/webapp-deploy-lambda"
  },
  "scripts": {
    "build": "./build.sh && tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "cdk",
    "cloudfront",
    "deployment",
    "s3",
    "webapp"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/webapp_deploy/**/*",
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.62.0",
    "@aws-cdk/aws-iam": "1.62.0",
    "@aws-cdk/aws-lambda": "1.62.0",
    "@aws-cdk/aws-s3": "1.62.0",
    "@aws-cdk/core": "1.62.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@liflig/cdk": "1.16.2",
    "@types/jest": "26.0.13",
    "@types/node": "13.13.16",
    "@typescript-eslint/eslint-plugin": "4.1.0",
    "@typescript-eslint/parser": "4.1.0",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-deprecation": "1.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "jest": "26.4.2",
    "jest-cdk-snapshot": "1.4.0",
    "prettier": "2.1.1",
    "semantic-release": "17.1.1",
    "ts-jest": "26.3.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "@aws-cdk/aws-iam": "^1.45.0",
    "@aws-cdk/aws-lambda": "^1.45.0",
    "@aws-cdk/aws-s3": "^1.45.0",
    "@aws-cdk/core": "^1.45.0"
  }
}
